{
	"env": {
		"browser": true,
		"es2015": true
	},
	"extends": [
		"plugin:react/recommended",
		"standard",
		"plugin:cypress/recommended",
		"prettier"
	],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 6,
		"sourceType": "module"
	},
	"plugins": [
		"react",
		"@typescript-eslint",
		"import-helpers",
		"cypress",
		"prettier"
	],
	"overrides": [{
		"files": "*.ts"
	}],
	"rules": {
		"react/prop-types": 0
	}
}
